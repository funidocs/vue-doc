{
	"vmodify": {
		"prefix": "vmodify",
		"body": [
			"<!--CCS系统-一级菜单名-二级菜单名-页面名称-->",
			"<template>",
			"\t<div class=\"applyDiv\">",
			"\t\t<a-card :bordered=\"false\">",
			"\t\t\t<funi-detail-head :option=\"detailHeadOption\"/>",
			"\t\t\t<a-card :bordered=\"false\">",
			"\t\t\t\t<a-steps class=\"applySteps\" :current=\"currentTab\">",
			"\t\t\t\t\t<a-step title=\"基本信息\"/>",
			"\t\t\t\t\t<a-step title=\"合同内容\"/>",
			"\t\t\t\t\t<a-step title=\"要件信息\"/>",
			"\t\t\t\t\t<a-step title=\"签约确认\"/>",
			"\t\t\t\t</a-steps>",
			"\t\t\t\t<div class=\"content\">",
			"<!--\t\t\t\t\t<step2 v-if=\"currentTab === 0\"/>-->",
			"<!--\t\t\t\t\t<step3 v-if=\"currentTab === 1\"/>-->",
			"<!--\t\t\t\t\t<step2 v-if=\"currentTab === 2\"/>-->",
			"<!--\t\t\t\t\t<step3 v-if=\"currentTab === 3\"/>-->",
			"\t\t\t\t</div>",
			"\t\t\t</a-card>",
			"\t\t\t<funi-process-bottom-btn :current-step=\"currentTab\" :submit-step=\"3\"",
			"\t\t\t\t\t\t\t\t\t\t\t\t\t\t\t @clickBtnEvent=\"_clickBtnEvent\"></funi-process-bottom-btn>",
			"\t\t</a-card>",
			"\t</div>",
			"</template>",
			"",
			"<script>",
			"\t// import Step1 from './Step1';",
			"\t// import Step2 from './Step2';",
			"\t// import Step3 from './Step3';",
			"",
			"\texport default {",
			"\t\tname: '${COMPONENT_NAME}',",
			"\t\tcomponents: {",
			"\t\t\t// Step1,",
			"\t\t\t// Step2,",
			"\t\t\t// Step3,",
			"\t\t},",
			"\t\tprops: {},",
			"\t\tcreated() {},",
			"\t\tdata() {",
			"\t\t\treturn {",
			"\t\t\t\tdetailHeadOption: {",
			"\t\t\t\t\trecordShow: false,",
			"\t\t\t\t\tno: '1234558',",
			"\t\t\t\t\ttitle: '存量房合同管理详情',",
			"\t\t\t\t\tstatus: '有效',",
			"\t\t\t\t},",
			"\t\t\t\tcurrentTab: 0,",
			"\t\t\t};",
			"\t\t},",
			"\t\tcomputed: {},",
			"\t\tmounted() {},",
			"\t\tmethods: {",
			"\t\t\t_clickBtnEvent(parmas) {",
			"\t\t\t\tswitch (parmas.type) {",
			"\t\t\t\t\tcase 'lastStep'://上一步",
			"\t\t\t\t\t\tthis.currentTab -= 1;",
			"\t\t\t\t\t\tbreak;",
			"\t\t\t\t\tcase 'nextStep'://下一步",
			"\t\t\t\t\t\tthis.currentTab += 1;",
			"\t\t\t\t\t\tbreak;",
			"\t\t\t\t\tcase 'ts'://暂存",
			"\t\t\t\t\t\tthis.$message.success('暂存成功');",
			"\t\t\t\t\t\tbreak;",
			"\t\t\t\t\tcase 'submit'://提交",
			"\t\t\t\t\t\tthis.currentTab += 1;",
			"\t\t\t\t\t\tbreak;",
			"\t\t\t\t}",
			"\t\t\t},",
			"\t\t},",
			"\t};",
			"</script>"						
		],
		"description": "vue 新增编辑模板"
	}
}